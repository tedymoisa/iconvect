{
  "name": "iconvect",
  "version": "1.1.1",
  "private": false,
  "description": "Generate stunning, unique vector icons instantly using the power of AI. Create scalable SVG assets for your projects in seconds.",
  "author": "kt",
  "license": "UNLICENSED",
  "homepage": "https://iconvect.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-kt/iconvect.git"
  },
  "keywords": [
    "ai",
    "svg",
    "vector",
    "icons",
    "generator",
    "graphics",
    "design",
    "ai generator",
    "nextjs",
    "react",
    "prisma",
    "trpc",
    "tailwindcss",
    "next-auth"
  ],
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "check:all": "pnpm run format:write && pnpm run typecheck && pnpm run lint"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@google/genai": "^0.14.1",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@t3-oss/env-nextjs": "^0.13.0",
    "@tanstack/react-query": "^5.74.4",
    "@trpc/client": "^11.1.1",
    "@trpc/react-query": "^11.1.1",
    "@trpc/server": "^11.1.1",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.5",
    "framer-motion": "^12.9.2",
    "geist": "^1.3.1",
    "html-react-parser": "^5.2.3",
    "isomorphic-dompurify": "^2.24.0",
    "lucide-react": "^0.503.0",
    "next": "^15.3.1",
    "next-auth": "5.0.0-beta.27",
    "next-themes": "^0.4.6",
    "openai": "^4.96.0",
    "posthog-js": "^1.236.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^2.0.3",
    "stripe": "^18.0.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@stripe/react-stripe-js": "^3.6.0",
    "@stripe/stripe-js": "^7.2.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.2",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.8.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "pnpm@10.6.5",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "sharp"
    ],
    "overrides": {
      "prismjs@<1.30.0": ">=1.30.0"
    }
  }
}
